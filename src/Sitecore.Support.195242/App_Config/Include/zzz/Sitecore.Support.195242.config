<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement">
    <commands>
      <command name="webedit:editlink"
               type="Sitecore.Support.XA.Foundation.Editing.Commands.EditLink, Sitecore.Support.195242"
               patch:instead="command[@name='webedit:editlink']" />
    </commands>

    <pipelines>
      <renderField>
        <processor type="Sitecore.Support.Pipelines.RenderField.GetLinkFieldValue, Sitecore.Support.195242" 
                   patch:instead="processor[@type='Sitecore.XA.Foundation.Multisite.Pipelines.RenderField.GetLinkFieldValue, Sitecore.XA.Foundation.Multisite']" />
      </renderField>
    </pipelines>
  </sitecore>
</configuration>